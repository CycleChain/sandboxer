{
    "name": "cyclechain/sandboxer",
    "description": "Zero-modification Laravel sandbox package for complete data isolation",
    "type": "library",
    "license": "MIT",
    "version": "0.2.0",
    "authors": [
        {
            "name": "Fatih Mert DoÄŸancan",
            "email": "fatih.dogancan@cyclechain.io",
            "homepage": "https://cyclechain.io"
        }
    ],
    "homepage": "https://github.com/cyclechain/sandboxer",
    "keywords": [
        "Laravel",
        "Sandboxer",
        "Sandbox",
        "Demo",
        "Isolation",
        "Testing"
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cyclechain\\Sandboxer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cyclechain\\Sandboxer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cyclechain\\Sandboxer\\SandboxerServiceProvider"
            ],
            "aliases": {
                "Sandboxer": "Cyclechain\\Sandboxer\\Facades\\Sandboxer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
